From f4eed08ea165aa7dd13ebf93be12c71b05d77ed2 Mon Sep 17 00:00:00 2001
From: Gary Zhang <b13634@freescale.com>
Date: Wed, 12 Sep 2012 10:03:11 +0800
Subject: [PATCH 2027/2463] ENGR00223911 wm8962: correct typo

correct typo for wm8962 description in Kconfig

Signed-off-by: Gary Zhang <b13634@freescale.com>
---
 sound/soc/imx/Kconfig |    4 ++--
 1 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/sound/soc/imx/Kconfig b/sound/soc/imx/Kconfig
index e4b57c7..72d8550 100644
--- a/sound/soc/imx/Kconfig
+++ b/sound/soc/imx/Kconfig
@@ -70,12 +70,12 @@ config SND_SOC_IMX_WM8958
 	  a WM8958 codec.
 
 config SND_SOC_IMX_WM8962
-	tristate "SoC Audio support for IMX boards with WM8958"
+	tristate "SoC Audio support for IMX boards with WM8962"
 	select SND_MXC_SOC_MX2
 	select SND_SOC_WM8962
 	help
 	  Say Y if you want to add support for SoC audio on an i.MX board with
-	  a WM8958 codec.
+	  a WM8962 codec.
 
 config SND_SOC_IMX_CS42888
 	tristate "SoC Audio support for i.MX boards with cs42888"
